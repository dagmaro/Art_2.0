<div>
  <h3>Gallery</h3>

  <div>
    {{#each availableNfts}}
      {{#if ../isUserActive}}
      <a href="/gallery/{{this._id}}/details">
        <h6>Name:{{this.name}}</h6>
        <img src="{{this.url}}" alt="nft-image" width="200px">
      </a>
      {{else}}
        <h6>Name:{{this.name}}</h6>
        <img src="{{this.url}}" alt="nft-image" width="200px">
      {{/if}}
    {{/each}}
  </div>

</div>